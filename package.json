{
  "name": "preact-audio-player",
  "version": "0.0.0",
  "description": "Audio player made with Preact and Preact Habitat",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "serve build -s -c 1 && node ./tools/start.cli.js",
    "prestart": "npm run build",
    "build": "cross-env webpack -p --progress && node ./tools/build.cli.js",
    "prebuild": "mkdirp build",
    "test": "npm run lint && npm run -s test:karma && node ./tools/test.cli.js",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "eslint ./src/ || true && node ./tools/lint.cli.js",
    "deploy": "git subtree push --prefix build origin gh-pages"
  },
  "keywords": [
    "preact",
    "audio-player"
  ],
  "license": "MIT",
  "author": "Adam Harris",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.2.0",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss-loader": "^1.2.1",
    "raw-loader": "^0.5.1",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "dependencies": {
    "left-pad": "^1.1.3",
    "preact": "^7.2.1",
    "preact-compat": "^3.0.0",
    "preact-habitat": "^2.0.0",
    "promise-polyfill": "^6.0.2",
    "proptypes": "^0.14.3",
    "serve": "^2.0.0",
    "yt-player": "^2.5.2"
  },
  "main": "webpack.config.babel.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agarrharr/preact-audio-player.git"
  },
  "bugs": {
    "url": "https://github.com/agarrharr/preact-audio-player/issues"
  },
  "homepage": "https://github.com/agarrharr/preact-audio-player#readme"
}
